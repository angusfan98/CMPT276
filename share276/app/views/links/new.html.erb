<h1>New Link</h1>

<%= form_with scope: :link, url: links_path, local: true do |form| %>
    <p>
      <%= form.label :URL %><br>
      <%= form.text_field :URL %>
    </p>
   
    <p>
      <%= form.label :Description %><br>
      <%= form.text_area :Description %>
    </p>
   
    <p>
      <%= form.submit %>
    </p>
  <% end %>

